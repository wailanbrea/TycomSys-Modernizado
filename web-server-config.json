{
  "web_server": {
    "type": "apache",
    "version": "2.4",
    "document_root": "public",
    "mod_rewrite": true,
    "allow_htaccess": true,
    "directory_index": ["index.php", "index.html"],
    "error_pages": {
      "404": "/index.php",
      "403": "/index.php",
      "500": "/index.php"
    },
    "virtual_hosts": [
      {
        "server_name": "ticomsys-modernizado-main-oawkq1.laravel.cloud",
        "document_root": "public",
        "directory_index": ["index.php", "index.html"],
        "mod_rewrite": true,
        "allow_htaccess": true
      }
    ]
  },
  "php": {
    "version": "8.2",
    "fpm": true,
    "extensions": ["mbstring", "openssl", "pdo", "pdo_pgsql", "tokenizer", "xml"]
  },
  "deployment": {
    "buildpack": "heroku/php",
    "entrypoint": "vendor/bin/heroku-php-apache2 public/",
    "processes": {
      "web": "vendor/bin/heroku-php-apache2 public/"
    }
  }
}
