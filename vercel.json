{
  "version": 2,
  "name": "ticomsys-moder",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/build",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://ticomsys-moder.vercel.app"
  }
}
