{
  "version": 2,
  "name": "ticomsys-moder",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://ticomsys-moder.vercel.app",
    "REACT_APP_API_URL": "https://ticomsys-moder.vercel.app/api"
  }
}
